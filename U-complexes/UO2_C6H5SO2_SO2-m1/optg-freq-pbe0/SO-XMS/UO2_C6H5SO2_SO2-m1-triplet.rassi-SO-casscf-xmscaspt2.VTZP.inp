/*
 Define geometry, basis set and point group
 Start the calculation with a minimal basis set
*/
 &GATEWAY
   COORD=$HomeDir/UO2_C6H5SO2_SO2-m1.pbe0.optg-zaxis.xyz
   BASIS=ANO-RCC-VTZP    
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD
>> COPY $Project.RunFile $HomeDir

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

>> COPY $HomeDir/UO2_C6H5SO2_SO2-m1-triplet.casscf.MB.RunFile RUNFIL1
>> COPY $Project.RunFile RUNFIL2
>> COPY $HomeDir/UO2_C6H5SO2_SO2-m1-triplet.casscf.MB.RasOrb_triplets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_triplets

>> COPY $HomeDir/UO2_C6H5SO2_SO2-m1-triplet.casscf.MB.RasOrb_singlets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_singlets

/*
 Perform an SCF calculation on the neutral molecule with a closed-shell ground state
 In Gaussian  98 alpha electrons       97 beta electrons
 Here we will have 60 electrons more cause we are doing an all-electron calculation
 &SCF
   Occupied=127
*/

/*
 Perform a CASSCF calculation on the real molecule
 1 active electron in 5 (2*5f_delta, 2*5f_phi, O peroxo) orbitals
*/

>> COPY $HomeDir/$Project.ExpOrb_triplets INPORB
 &RASSCF
   Spin=3
   Symmetry=1
   Nactel=2 0 0
   Inactive=106
   Ras2=5
   CIRoots=4 4 1
   Lumorb
>> COPY $Project.JobIph $HomeDir/$Project.JobIph_triplets
>> COPY $Project.RasOrb $HomeDir/$Project.RasOrb_triplets

/*
  Perform a XMS-CASPT2 calculation for the triplet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=150
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $HomeDir/$Project.JobMix_triplets

>> COPY $HomeDir/$Project.ExpOrb_singlets INPORB
 &RASSCF
   Spin=1
   Symmetry=1
   Nactel=2 0 0
   Inactive=106
   Ras2=5
   CIRoots=4 4 1
>> COPY $Project.JobIph $HomeDir/$Project.JobIph_singlets
>> COPY $Project.RasOrb $HomeDir/$Project.RasOrb_singlets

/*
  Perform a XMS-CASPT2 calculation for the singlet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=150
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $HomeDir/$Project.JobMix_singlets

/*
  Perform a RASSI-SO calculation for the CASSCF triplets and singlets F states (JobIph file)
*/
>> COPY $HomeDir/$Project.JobIph_triplets JOB001
>> COPY $HomeDir/$Project.JobIph_singlets JOB002
  &RASSI
    EJob
    SPINorbit

/*
  Perform a RASSI-SO calculation for the XMS-CASPT2 triplets and singlets F states (JobMix file)
*/
>> COPY $HomeDir/$Project.JobMix_triplets JOB001
>> COPY $HomeDir/$Project.JobMix_singlets JOB002
  &RASSI
    EJob
    SPINorbit
