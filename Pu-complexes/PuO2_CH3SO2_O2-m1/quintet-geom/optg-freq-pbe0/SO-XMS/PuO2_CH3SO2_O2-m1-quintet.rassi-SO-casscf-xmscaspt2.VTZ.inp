 &GATEWAY
   COORD=$HomeDir/PuO2_CH3SO2_O2-m1-quintet.pbe0.optg-zaxis.xyz
   BASIS=ANO-RCC-VTZP
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

>> COPY $HomeDir/PuO2_CH3SO2_O2-m1-quintet.casscf.MB.RunFile RUNFIL1
>> COPY $Project.RunFile RUNFIL2
>> COPY $HomeDir/PuO2_CH3SO2_O2-m1-quintet.casscf.MB.RasOrb_quintets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_quintets

>> COPY $HomeDir/PuO2_CH3SO2_O2-m1-quintet.casscf.MB.RasOrb_triplets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_triplets

>> COPY $HomeDir/PuO2_CH3SO2_O2-m1-quintet.casscf.MB.RasOrb_singlets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_singlets

>> COPY $HomeDir/$Project.ExpOrb_quintets INPORB
 &RASSCF
   Spin=5
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=8 8 1
   Lumorb
>> COPY $Project.JobIph $Project.JobIph_quintets

/*
  Perform a XMS-CASPT2 calculation for the quintets F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=100
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_quintets


>> COPY $HomeDir/$Project.ExpOrb_triplets INPORB
 &RASSCF
   Spin=3
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=16 16 1
   Lumorb
>> COPY $Project.JobIph $Project.JobIph_triplets

/*
  Perform a XMS-CASPT2 calculation for the triplet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=100
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_triplets


>> COPY $HomeDir/$Project.ExpOrb_singlets INPORB
 &RASSCF
   Spin=1
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=16 16 1
   Lumorb
>> COPY $Project.JobIph $Project.JobIph_singlets

/*
  Perform a XMS-CASPT2 calculation for the singlet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=100
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_singlets

/*
  Perform a RASSI-SO calculation for the CASSCF triplets and doublets F states (JobIph file)
*/
>> COPY $Project.JobIph_quintets JOB001
>> COPY $Project.JobIph_triplets JOB002
>> COPY $Project.JobIph_singlets JOB003
  &RASSI
    EJob
    SPINorbit

/*
  Perform a RASSI-SO calculation for the XMS-CASPT2 triplets and doublets F states (JobMix file)
*/
>> COPY $Project.JobMix_quintets JOB001
>> COPY $Project.JobMix_triplets JOB002
>> COPY $Project.JobMix_singlets JOB003
  &RASSI
    EJob
    SPINorbit
