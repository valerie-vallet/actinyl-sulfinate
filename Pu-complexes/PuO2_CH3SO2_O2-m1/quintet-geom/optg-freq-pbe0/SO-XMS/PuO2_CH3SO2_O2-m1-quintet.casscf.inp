/*
 Define geometry, basis set and point group
 Start the calculation with a minimal basis set
*/
 &GATEWAY
   COORD=$HomeDir/PuO2_CH3SO2_O2-m1-quintet.pbe0.optg.xyz
   BASIS=ANO-RCC-MB    
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD
>> COPY $Project.RunFile RUNFIL1

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

/*
 Perform an SCF calculation on the neutral molecule with a closed-shell ground state
 In Gaussian  98 alpha electrons       97 beta electrons
 Here we will have 60 electrons more cause we are doing an all-electron calculation
 &SCF
   Occupied=127
*/

/*
 Perform a CASSCF calculation on the real molecule
 1 active electron in 5 (2*5f_delta, 2*5f_phi, O peroxo) orbitals
*/
 &RASSCF
   Spin=5
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=20 20 1
>> COPY $Project.RasOrb $Project.RasOrb_quintets_MB

 &RASSCF
   Spin=3
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=20 20 1
>> COPY $Project.RasOrb $Project.RasOrb_triplets_MB

 &RASSCF
   Spin=1
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=20 20 1
>> COPY $Project.RasOrb $Project.RasOrb_singlets_MB

 &GATEWAY
   COORD=$HomeDir/PuO2_CH3SO2_O2-m1-quintet.pbe0.optg.xyz
   BASIS=ANO-RCC-VTZP
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD
>> COPY $Project.RunFile RUNFIL2

>> COPY $Project.RasOrb_quintets_MB INPORB
 &EXPBAS &END
>> COPY  $Project.ExpOrb INPORB

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD
/*
 Perform a CASSCF calculation on the real molecule
 1 active electron in 4 (2*5f_delta, 2*5f_phi) orbitals
*/
 &RASSCF
   Spin=5
   Symmetry=1
   Nactel=6 0 0
   Inactive=81
   Ras2=6
   CIRoots=4 4 1
>> COPY $Project.RasOrb $Project.RasOrb_quintets_VTZP
