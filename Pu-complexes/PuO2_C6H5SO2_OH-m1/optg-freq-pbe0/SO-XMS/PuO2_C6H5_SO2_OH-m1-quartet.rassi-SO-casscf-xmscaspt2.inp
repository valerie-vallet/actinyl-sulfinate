 &GATEWAY
   COORD=$HomeDir/PuO2_C6H5_SO2_OH-quartet.pbe0.optg.XYZ
   BASIS=ANO-RCC-VTZP
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

>> COPY $HomeDir/PuO2_C6H5_SO2_OH-m1-quartet.casscf.ExpOrb INPORB
 &RASSCF
   Spin=4
   Symmetry=1
   Nactel=3 0 0
   Inactive=95
   Ras2=4
   CIRoots=4 4 1
>> COPY $Project.JobIph $Project.JobIph_quartets

/*
  Perform a XMS-CASPT2 calculation for the quartets F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=100
    CONVERGENCE=1.0D-5
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_quartets


>> COPY $HomeDir/PuO2_C6H5_SO2_OH-m1-quartet.casscf.ExpOrb INPORB
 &RASSCF
   Spin=2
   Symmetry=1
   Nactel=3 0 0
   Inactive=95
   Ras2=4
   CIRoots=20 20 1
>> COPY $Project.JobIph $Project.JobIph_doublets

/*
  Perform a XMS-CASPT2 calculation for the triplet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=100
    CONVERGENCE=1.0D-5
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_doublets


/*
  Perform a RASSI-SO calculation for the CASSCF doublets and doublets F states (JobIph file)
*/
>> COPY $Project.JobIph_quartets JOB001
>> COPY $Project.JobIph_doublets JOB002
  &RASSI
    EJob
    SPINorbit

/*
  Perform a RASSI-SO calculation for the XMS-CASPT2 doublets and doublets F states (JobMix file)
*/
>> COPY $Project.JobMix_quartets JOB001
>> COPY $Project.JobMix_doublets JOB002
  &RASSI
    EJob
    SPINorbit
