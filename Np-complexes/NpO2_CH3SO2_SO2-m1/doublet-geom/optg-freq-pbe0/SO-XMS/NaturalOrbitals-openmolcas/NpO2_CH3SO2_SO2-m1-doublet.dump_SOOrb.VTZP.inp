 &GATEWAY
   COORD=$HomeDir/NpO2_CH3SO2_SO2-m1-doublet.pbe0.optg-zaxis.xyz
   BASIS=ANO-RCC-VTZP, Np.ANO-RCC...9s8p6d4f2g.
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

>> COPY $HomeDir/NpO2_CH3SO2_SO2-m1-doublet.casscf.MB.RunFile RUNFIL1
>> COPY $Project.RunFile RUNFIL2
>> COPY $HomeDir/NpO2_CH3SO2_SO2-m1-doublet.casscf.MB.RasOrb_quartets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_quartets

>> COPY $HomeDir/NpO2_CH3SO2_SO2-m1-doublet.casscf.MB.RasOrb_doublets INPORB
 &EXPBAS &END
>> COPY $Project.ExpOrb $HomeDir/$Project.ExpOrb_doublets

>> COPY $HomeDir/$Project.ExpOrb_quartets INPORB
 &RASSCF
   Spin=4
   Symmetry=1
   Nactel=3 0 0
   Inactive=90
   Ras2=5
   CIRoots=6 6 1
   Lumorb
>> COPY $Project.JobIph $Project.JobIph_quartets
>> COPY $Project.RasOrb $Project.RasOrb_quartets

/*
  Perform a XMS-CASPT2 calculation for the quartet F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=150
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_quartets



>> COPY $HomeDir/$Project.ExpOrb_doublets INPORB
 &RASSCF
   Spin=2
   Symmetry=1
   Nactel=3 0 0
   Inactive=90
   Ras2=5
   CIRoots=12 12 1
   Lumorb
>> COPY $Project.JobIph $Project.JobIph_doublets

/*
  Perform a XMS-CASPT2 calculation for the doublets F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=150
    CONVERGENCE=1.0D-6
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_doublets

/*
  Perform a RASSI-SO calculation for the XMS-CASPT2 quartet and doublets F states (JobMix file)
*/
&RASSI &End
    EJob
    SPINorbit
    Omega
    SPIN
    EPRG
    1.0
    MEES
    PROPerties
    3
    'AngMom' 1
    'AngMom' 2
    'AngMom' 3
    SONORB
    2
    1
    2
    SODIAG
    2
    1
    2

>> COPY $HomeDir/$Project.SODSOrb.Z.Z1 INPORB
&RASSCF &End
   Spin=3
   Symmetry=1
   Nactel=4 0 0
   Inactive=90
   Ras2=5
   CIRoots=1 1; 1
   LUMORB
   CIONLY
>> COPY $Project.rasscf.molden $HomeDir/$Project.SODSOrb.Z.Z1.molden

