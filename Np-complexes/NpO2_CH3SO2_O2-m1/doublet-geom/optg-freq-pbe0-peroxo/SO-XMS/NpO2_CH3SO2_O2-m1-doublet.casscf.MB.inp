/*
 Define geometry, basis set and point group
 Start the calculation with a minimal basis set
*/
 &GATEWAY
   COORD=$HomeDir/NpO2_CH3SO2_O2-m1-doublet.pbe0.optg-zaxis.xyz
   BASIS=ANO-RCC-MB    
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD
>> COPY $Project.RunFile $HomeDir

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

/*
 Perform a CASSCF calculation on the real molecule
 3 active electrons in 4 (2*5f_delta, 2*5f_phi) orbitals + 2 peroxide orbitals
*/
 &RASSCF
   Spin=4
   Symmetry=1
   Nactel=5 0 0
   Inactive=81
   Ras2=6
   CIRoots=1 1 1
   Lumorb

>> COPY $Project.RasOrb INPORB
 &RASSCF
   Spin=4
   Symmetry=1
   Nactel=5 0 0
   Inactive=81
   Ras2=6
   CIRoots=12 12 1
   ALTER=1; 1 80 83;
   Lumorb
>> COPY $Project.RasOrb $HomeDir/$Project.RasOrb_quartets
>> COPY $Project.rasscf.molden $HomeDir/$Project.rasscf_quartets.molden

>> COPY $Project.RasOrb INPORB
 &RASSCF
   Spin=2
   Symmetry=1
   Nactel=5 0 0
   Inactive=81
   Ras2=6
   CIRoots=24 24 1
   Lumorb
>> COPY $Project.RasOrb $HomeDir/$Project.RasOrb_doublets
>> COPY $Project.rasscf.molden $HomeDir/$Project.rasscf_doublets.molden

