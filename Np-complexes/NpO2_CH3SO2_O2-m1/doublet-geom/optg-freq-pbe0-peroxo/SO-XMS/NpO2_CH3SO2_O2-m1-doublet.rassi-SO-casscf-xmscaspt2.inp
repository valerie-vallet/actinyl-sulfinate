 &GATEWAY
   COORD=$HomeDir/NpO2_CH3SO2_O2-m1-doublet.pbe0.optg.xyz
   BASIS=ANO-RCC-VTZP
   Group=Nosym
   ANGM=0.0 0.0 0.0
   RICD

/*
  Compute integrals with Cholesky decomposition
*/
 &SEWARD

>> COPY $HomeDir/NpO2_CH3SO2_O2-m1-doublet.casscf.RasOrb INPORB
 &RASSCF
   Spin=4
   Symmetry=1
   Nactel=5 0 0
   Inactive=81
   Ras2=6
   CIRoots=6 6 1
>> COPY $Project.JobIph $Project.JobIph_quartets

/*
  Perform a XMS-CASPT2 calculation for the quartets F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=150
    CONVERGENCE=1.0D-5
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_quartets



>> COPY $HomeDir/NpO2_CH3SO2_O2-m1-doublet.casscf.ExpOrb INPORB
 &RASSCF
   Spin=2
   Symmetry=1
   Nactel=5 0 0
   Inactive=81
   Ras2=6
   CIRoots=12 12 1
>> COPY $Project.JobIph $Project.JobIph_doublets

/*
  Perform a XMS-CASPT2 calculation for the doublets F states
*/
  &CASPT2
    IPEA=0.0
    XMUL= ALL
    THRESHOLDS= 1.0D-09 1.0D-07
    MAXITER=250
    CONVERGENCE=1.0D-5
    IMAG=0.1
>> COPY $Project.JobMix $Project.JobMix_doublets

/*
  Perform a RASSI-SO calculation for the CASSCF quartets and doublets F states (JobIph file)
*/
>> COPY $Project.JobIph_quartets JOB001
>> COPY $Project.JobIph_doublets JOB002
  &RASSI
    EJob
    SPINorbit

/*
  Perform a RASSI-SO calculation for the XMS-CASPT2 quartets and doublets F states (JobMix file)
*/
>> COPY $Project.JobMix_quartets JOB001
>> COPY $Project.JobMix_doublets JOB002
  &RASSI
    EJob
    SPINorbit
